{
  "name": "vulk",
  "version": "1.3.0",
  "private": true,
  "license": "SEE LICENSE IN LICENSE.md",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-s clean build:*",
    "build:update-bulma-colors": "bulma-css-vars",
    "build:vite": "vite build",
    "serve": "serve -s dist -p 3000",
    "ssr:dev": "esno server.ts",
    "ssr:build": "run-s clean build:update-bulma-colors ssr:build:*",
    "ssr:build:client": "vite build --ssrManifest --outDir dist/client",
    "ssr:build:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
    "ssr:serve": "cross-env NODE_ENV=production esno server.ts",
    "ssg:build": "pnpm build:update-bulma-colors && cross-env NODE_ENV=production esno build-ssg.ts",
    "ssg:serve": "serve dist -p 3000",
    "clean": "rimraf dist",
    "test": "run-p test:*",
    "test:tsc": "vue-tsc --noEmit",
    "test:lint": "run-s lint:eslint lint:stylelint",
    "lint": "run-s lint:eslint:fix lint:stylelint:fix",
    "lint:eslint": "eslint --ext .ts,.vue ./src",
    "lint:eslint:fix": "eslint --fix --ext .ts,.vue ./src",
    "lint:stylelint": "stylelint ./src/**/*.{vue,css,scss}",
    "lint:stylelint:fix": "stylelint ./src/**/*.{vue,css,scss} --fix"
  },
  "dependencies": {
    "@iconify/iconify": "3.1.0",
    "@nuxt/devalue": "2.0.0",
    "@vueform/slider": "2.1.7",
    "@vueuse/components": "9.13.0",
    "@vueuse/core": "9.13.0",
    "@vueuse/head": "1.0.26",
    "@vueuse/integrations": "9.13.0",
    "axios": "1.3.3",
    "bulma": "0.9.4",
    "cobe": "0.6.3",
    "compression": "1.7.4",
    "date-fns": "2.29.3",
    "mapbox-gl": "2.12.1",
    "nprogress": "0.2.0",
    "pinia": "2.0.30",
    "plyr": "3.7.3",
    "prism-theme-vars": "0.2.4",
    "prismjs": "1.29.0",
    "tslib": "2.5.0",
    "universal-cookie": "4.0.4",
    "vue": "3.2.47",
    "vue-accessible-color-picker": "4.1.2",
    "vue-demi": "0.13.11",
    "vue-i18n": "9.2.2",
    "vue-marquee-text-component": "2.0.1",
    "vue-my-photos": "3.0.1",
    "vue-prism-component": "2.0.0",
    "vue-scrollto": "2.20.0",
    "vue3-carousel": "0.2.9",
    "vue3-markdown-it": "1.0.10",
    "vue3-popper": "1.5.0",
    "vue3-scroll-spy": "1.0.8"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@cssninja/bulma-css-vars": "0.9.2",
    "@iconify/json": "2.2.24",
    "@intlify/unplugin-vue-i18n": "0.8.2",
    "@types/mapbox-gl": "2.7.10",
    "@types/node": "18.14.0",
    "@types/nprogress": "0.2.0",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "@vitejs/plugin-vue": "4.0.0",
    "autoprefixer": "10.4.13",
    "commitlint": "17.4.4",
    "cross-env": "7.0.3",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier-vue": "4.2.0",
    "eslint-plugin-vue": "9.9.0",
    "eslint-plugin-vuejs-accessibility": "2.1.0",
    "esno": "0.16.3",
    "fast-glob": "^3.2.12",
    "h3": "1.5.0",
    "jiti": "1.17.1",
    "lint-staged": "13.1.2",
    "listhen": "1.0.3",
    "npm-run-all": "4.1.5",
    "postcss-html": "1.5.0",
    "postcss-scss": "4.0.6",
    "prettier": "2.8.4",
    "rimraf": "4.1.2",
    "sass": "1.58.3",
    "serve": "^14.2.0",
    "serve-static": "1.15.0",
    "stylelint": "14.16.1",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-recommended-vue": "1.4.0",
    "stylelint-config-standard": "29.0.0",
    "stylelint-config-standard-scss": "6.1.0",
    "stylelint-prettier": "2.0.0",
    "stylelint-scss": "4.4.0",
    "typescript": "4.9.5",
    "unplugin-auto-import": "^0.14.3",
    "unplugin-icons": "0.15.3",
    "unplugin-vue-components": "0.24.0",
    "vite": "4.1.2",
    "vite-plugin-fonts": "0.7.0",
    "vite-plugin-pages": "0.28.0",
    "vite-plugin-pages-sitemap": "^1.4.5",
    "vite-plugin-purge-icons": "0.9.2",
    "vite-plugin-pwa": "0.14.4",
    "vite-plugin-radar": "0.7.0",
    "vue-router": "4.1.6",
    "vue-tsc": "1.1.4",
    "yorkie": "2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -e -V "
  },
  "lint-staged": {
    "*.{ts,js,vue}": [
      "eslint --fix"
    ],
    "*.scss": [
      "stylelint"
    ]
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@types/markdown-it",
        "postcss",
        "stylelint"
      ]
    }
  }
}
